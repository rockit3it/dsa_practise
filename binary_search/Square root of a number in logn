class Solution
{
     long floorSqrt(long x)
	 {
		// Your code here
		long start = 1;
		long end = x;
		long max = Integer.MIN_VALUE;
		while(start <= end){
		    long mid = start + (end-start)/2;
		    if(mid*mid == x){
		        return mid;
		    }
		    if(mid*mid > x){
		        end = mid-1;
		    }
		    else{
		        max = Math.max(max, mid);
		        start = mid+1;
		    }
		}
		return max;
	 }
}
